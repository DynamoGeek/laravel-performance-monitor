{
  "name": "dealerinspire/laravel-performance-monitor",
  "description": "A performance monitor to log an error when a request performs below a specified threshold",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.8.*",
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "DealerInspire\\LaravelPerformanceMonitor\\": "./src",
      "DealerInspire\\LaravelPerformanceMonitor\\Tests\\": "./tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DealerInspire\\LaravelPerformanceMonitor\\PerformanceMonitorServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.5"
  }
}
