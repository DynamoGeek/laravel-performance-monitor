{
  "name": "dealer-inspire/laravel-performance-monitor",
  "description": "A performance monitor to log an error when a request performs below a specified threshold",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.8.*"
  },
  "autoload": {
    "psr-4": {
      "DealerInspire\\LaravelPerformanceMonitor\\": "./src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DealerInspire\\LaravelPerformanceMonitor\\PerformanceMonitorServiceProvider"
      ]
    }
  }
}